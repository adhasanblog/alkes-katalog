{
  "name": "alkes-catalog",
  "version": "1.0.0",
  "lint-staged": {
    "*.php": [
      "node scripts/phpcbf-staged.js",
      "php vendor/bin/phpcs --standard=phpcs.xml.dist"
    ],
    "assets/src/**/*.{js,mjs,cjs,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint:php": "composer lint:php",
    "lint:php:fix": "composer lint:php:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint": "npm run lint:php && npm run lint:js",
    "dev": "vite build --watch",
    "build": "vite build",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "autoprefixer": "^10.4.24",
    "motion": "^12.31.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.5",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "semantic-release": "^25.0.3"
  }
}
